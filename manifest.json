{
    "name": "Bar Begone",
    "description": "Remove the progress bar and timestamp on YouTube videos.",
    "version": "1.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*",
                "*://*.youtu.be.com/*"
            ],
            "css": ["style.css"],
            "run_at": "document_end"
        }
    ]
}
